{
  "name": "web-pdm",
  "version": "0.2.3-dev.2",
  "description": "一个用G6做的ER图工具，最终目标是想做成在线版的 powerdesigner",
  "main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "es"
  ],
  "scripts": {
    "start": "dumi dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf public &&  NODE_ENV=production webpack --config ./webconfig.config.js --mode production  --colors --max_old_space_size=1024 && rm -rf docker/public  && cp -r ./public docker/public",
    "watch": "time webpack-dev-server  --watch-poll --config ./webconfig.config.js --mode development",
    "publish-npm": "npm run build-lib ; npm publish --registry=http://registry.npmjs.org",
    "publish-docker": "cd ./docker && sh build.sh",
    "publish-server": "cd ./docker && sh upload.sh",
    "dev": "father doc dev",
    "build-lib": "npm run copy && npm run tsc",
    
    "copy": "rm -rf es && mkdir es && cp -R src/* es && find es -name '*.ts' | xargs rm -f && find es -name '*.tsx' | xargs rm -f",
    "dist": "rm -rf dist && TS_NODE_PROJECT=\"./tsconfig-for-webpack-config.json\" webpack --config webpack.config.ts --mode development",
    "tsc": "tsc src/*.tsx  src/**/*.tsx  --allowSyntheticDefaultImports true -d --lib es6,dom --experimentalDecorators true -t es6 --moduleResolution node --jsx react --outDir es"
  },
  "author": "北名",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.8.4",
    "@umijs/plugin-antd": "^0.8.0",
    "@umijs/plugin-sass": "^1.1.1",
    "ak-webpack": "^1.0.15",
    "copyfiles": "^2.3.0",
    "father": "^2.29.9",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^3.8.2",
    "wrappy": "^1.0.2",
    "xdg-basedir": "^4.0.0",
    "xtend": "^4.0.2"
  },
  "dependencies": {
    "@antv/g6": "^3.6.0",
    "@iarna/rtf-to-html": "^1.1.0",
    "antd": "^4.6.2",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.14",
    "immer": "^4.0.0",
    "mobx": "^5.15.5",
    "mobx-keystone": "^0.46.0",
    "mobx-react-lite": "^2.0.7",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^9.3.4",
    "react-dnd-html5-backend": "^9.3.4",
    "react-dom": "^16.9.0",
    "react-scrollbars-custom": "^4.0.21",
    "with-dnd-context": "^1.0.4",
    "xml2js": "^0.4.23"
  }
}
